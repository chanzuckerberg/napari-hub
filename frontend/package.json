{
  "name": "napari-hub",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "// Build frontend for production.": "",
    "build": "next build",
    "// Deprecated. Use yarn preview:build": "",
    "build:preview": "yarn preview:build",
    "// Run frontend in dev mode at http://localhost:8080": "",
    "dev": "nodemon -x 'run-p dev:*' -w tailwind.config.js",
    "// Runs mock server. Use MOCK_SERVER=false to disable server for `yarn dev`": "",
    "dev:api": "nodemon mock-server",
    "// Runs next.js server in dev mode on port 8080": "",
    "dev:next": "next dev -p 8080",
    "// Runs Typed SCSS Modules CLI in watch mode.": "",
    "dev:tsm": "tsm -w -e default -p.@ src 'src/**/*.module.scss'",
    "// Runs all linters": "",
    "lint": "run-p lint:*",
    "lint:prettier": "prettier --check .",
    "lint:stylelint": "stylelint '**/*.scss'",
    "lint:eslint": "run-p lint:client lint:src",
    "lint:client": "eslint --ignore-pattern src .",
    "lint:src": "eslint src",
    "// Runs e2e tests. Use SCREEN to run test for specific screen size.": "",
    "e2e": "jest -c jest/e2e.config.js",
    "// Runs e2e tests in watch mode": "",
    "e2e:watch": "yarn e2e --watch",
    "// Update e2e test snapshots": "",
    "e2e:update": "yarn e2e --updateSnapshot",
    "// Runs postinstall script to setup husky on `yarn install`": "",
    "postinstall": "node postinstall",
    "// Builds the preview page. Requires PREVIEW and PREVIEW_PULL_REQUEST env vars": "",
    "preview:build": "run-s preview:build:*",
    "// Builds frontend server for production": "",
    "preview:build:next": "next build",
    "// Exports preview page as static HTML to preview/ directory": "",
    "preview:build:export": "next export -o preview",
    "// Renames the `preview.html` file to `index.html`": "",
    "preview:build:rename-files": "mv preview/preview.html preview/index.html",
    "// Runs preview page in dev mode. Accessible from http://localhost:8080/preview": "",
    "preview:dev": "PREVIEW=src/fixtures/preview.json PREVIEW_PULL_REQUEST=https://github.com/chanzuckerberg/napari-demo/pull/5 yarn dev",
    "// Builds preview page with dev data.": "",
    "preview:dev:build": "PREVIEW=src/fixtures/plugin.json PREVIEW_PULL_REQUEST=https://github.com/chanzuckerberg/napari-demo/pull/5 yarn preview:build",
    "// Runs frontend in production. Requires running `yarn build` first.": "",
    "start": "next start -p 8080",
    "// Runs unit / integration tests": "",
    "test": "jest -c jest/test.config.js",
    "// Runs unit / integration tests in watch mode": "",
    "test:watch": "yarn test --watch",
    "// Update test snapshots ": "",
    "test:update": "yarn test --updateSnapshot",
    "// Type checks codebase with TypeScript": "",
    "type-check": "tsc -p tsconfig.json --noEmit"
  },
